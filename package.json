  "repository": {
    "type": "git",
    "url": "https://github.com/DorukEmre/drug-interaction-check.git"
  },
  "dependencies": {
    "axios": "^1.5.1",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.18",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.8.2",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  }
}
