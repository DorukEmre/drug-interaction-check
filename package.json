{
  "name": "drug-interaction-check",
  "version": "1.0.0",
  "description": "Drug interaction checker app",
  "private": "true",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "compile": "tsc",
    "watch": "tsc -w",
    "start": "npm run compile && node dist/index.js",
    "server": "nodemon dist/index.js",
    "client": "npm run dev --prefix client",
    "servertest": "jest --testPathPattern=server",
    "clienttest": "jest --testPathPattern=client"
  },
